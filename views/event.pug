


extends ./layout.pug

block contenido
        div
            nav.navbar.navbar-light.navbar-expand-md.navigation-clean(style='background-color: rgb(244,216,71);')
                .container
                    a.navbar-brand(href='event', style="font-family: 'Sawarabi Gothic', sans-serif;") MEETUS
                    button.navbar-toggler(data-toggle='collapse', data-target='#navcol-1')
                        span.sr-only Toggle navigation
                        span.navbar-toggler-icon
                    #navcol-1.collapse.navbar-collapse
                        ul.nav.navbar-nav.ml-auto
